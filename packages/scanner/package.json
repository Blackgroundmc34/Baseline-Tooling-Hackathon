{
  "name": "scanner",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "bin": { "baseline-compat": "dist/cli.js" },
  "scripts": {
    "scan": "ts-node --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "scan:nohtml": "ts-node --transpile-only src/index.ts -- --no-html",
    "report": "ts-node --transpile-only src/report-html.ts",
