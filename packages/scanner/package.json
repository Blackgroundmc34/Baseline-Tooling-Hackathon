    "fast-glob": "^3.3.3",
    "globby": "^15.0.0",
    "parse5": "^8.0.0",
    "postcss": "^8.5.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@mdn/browser-compat-data": "^7.1.10",
    "compute-baseline": "^0.4.0",
    "web-features": "^3.3.0"
